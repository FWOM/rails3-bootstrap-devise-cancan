<%= simple_form_for(@orgtree) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :parent %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :image %>
    <%= f.input :phone %>
    <%= f.input :email %>
    <%= f.input :templateName %>
    <%= f.input :href %>
    <%= f.input :itemTitleColor %>
    <%= f.input :user_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
